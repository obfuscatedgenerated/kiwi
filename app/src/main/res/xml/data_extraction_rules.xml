<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- backup preferences to the cloud -->
        <include domain="file" path="datastore/kiwiPrefs.preferences_pb" />
    </cloud-backup>
    <device-transfer>
        <!-- transfer preferences to a new device -->
        <include domain="file" path="datastore/kiwiPrefs.preferences_pb" />
    </device-transfer>
</data-extraction-rules>